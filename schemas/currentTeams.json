{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "minItems": 2,
  "maxItems": 2,
  "items": {
    "type": "object",
    "properties": {
      "name": { "type": "string" },
      "data": { "type": "object", "additionalProperties": true }
    }
  },
  "default": [
    {
      "name": "Team A",
      "data": {
        "roster": ["Player A1", "Player A2", "Player A3", "Player A4"]
      }
    },
    {
      "name": "Team B",
      "data": {
        "roster": ["Player B1", "Player B2", "Player B3", "Player B4"]
      }
    }
  ]
}
