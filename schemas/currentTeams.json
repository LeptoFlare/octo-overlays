{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "minItems": 2,
  "maxItems": 2,
  "items": {
    "type": "object",
    "properties": {
      "name": { "type": "string" },
      "data": {
        "type": ["object", "array", "null"],
        "additionalProperties": true
      }
    }
  },
  "default": [
    {
      "name": "Team A",
      "data": null
    },
    {
      "name": "Team B",
      "data": null
    }
  ]
}
